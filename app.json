{
  "expo": {
    "name": "NavoApp",
    "slug": "navoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tmarcus68.navoapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location services to provide location tracking.",
        "NSLocationAlwaysUsageDescription": "This app needs to access your location in the background."
      }
    },
    "android": {
      "package": "com.tmarcus68.navoapp",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_BACKGROUND_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
